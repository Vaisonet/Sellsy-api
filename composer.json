{
  "name": "ferjul17/sellsy-api",
  "type": "library",
  "description": "PHP library which helps to call Sellsy's API",
  "keywords": [
    "library",
    "sellsy",
    "api"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Julien Ferrier",
      "email": "ferjul17@gmail.com",
      "homepage": "https://github.com/ferjul17"
    }
  ],
  "require": {
    "guzzlehttp/guzzle": "^6.0",
    "psr/log": "^1.0",
    "spatie/guzzle-rate-limiter-middleware": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.0"
  },
  "autoload": {
    "psr-4": {
